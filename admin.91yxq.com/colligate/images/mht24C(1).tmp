<SCRIPT type=text/javascript>
function drawlinegraph(charid,type,title,xData,yData,subtitle,ytitle,swidth){
var chart;
$(document).ready(function() {
   chart = new Highcharts.Chart({
      chart: {
         renderTo: charid,
         defaultSeriesType: type,
         width:swidth,
         height:180,
         //marginbottom:-5,
         //margintop:-5
         //borderwidth:5
         margin: [50, 50, 20, 50]

      },
      colors: [
	'#B4EE64', 
	'#ff8844', 
	'#92A8CD', 
	'#80699B', 
	'#3D96AE', 
	'#DB843D', 
	'#92A8CD', 
	'#A47D7C', 
	'#B5CA92'
],
      title: {
         text: title,
         style: {font: 'normal 12px Arial, sans-serif'} 
      },
      subtitle: {
         align:'right',
         text:subtitle
      },
      xAxis: {
         categories: xData,
         labels: {rotation: -0,align: 'center',color:'black',style: {font: 'normal 9px Arial, sans-serif'}}
      },
      yAxis: {
         title: {
            text: ytitle
         }
      },
      legend: {
         layout: 'vertical',
         align: 'left',
         verticalAlign:'top',
         x: 0,
         y: -8,
         borderWidth: 0
      },
      credits:{
      enabled:false
     },
      exporting:{
        enabled:false,
        url:'admin/interface/reg_stat_channel_hour_v.php'
        },

      tooltip: {
         formatter: function() {
            return '<b>'+ this.series.name +'</b><br/>'+
               this.x +': '+ this.y + (this.series.name == '流失率'  ? ' %': ' 人');
         }
      },
      series: yData 
   });
   
   
});
}
